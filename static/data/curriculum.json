  "Psychology": {
    "Title": "AQA A-Level Psychology (7182)",
    "Papers": [
      {
        "Name": "Paper 1",
        "Title": "Introductory Topics in Psychology",
        "Topics": [
          {
            "Title": "Social Influence",
            "Content": [
              "Types of conformity (compliance, identification, internalisation)",
              "Explanations for conformity (ISI and NSI)",
              "Asch's research on conformity",
              "Zimbardo's Stanford Prison Experiment",
              "Milgram's research on obedience",
              "Agentic state and legitimacy of authority",
              "Resistance to social influence",
              "Minority influence and social change"
            ]
          },
          {
            "Title": "Memory",
            "Content": [
              "Multi-store model of memory (MSM)",
              "Working memory model (WMM)",
              "Types of long-term memory (episodic, procedural, semantic)",
              "Forgetting (interference, retrieval failure)",
              "Eyewitness testimony (misleading information, anxiety)",
              "Cognitive interview"
            ]
          },
          {
            "Title": "Attachment",
            "Content": [
              "Caregiver-infant interactions",
              "Schaffer's stages of attachment",
              "Animal studies of attachment (Lorenz, Harlow)",
              "Learning theory vs. Bowlby's monotropic theory",
              "Ainsworth's Strange Situation",
              "Cultural variations in attachment",
              "Bowlby's maternal deprivation hypothesis",
              "Effects of institutionalisation"
            ]
          },
          {
            "Title": "Psychopathology",
            "Content": [
              "Definitions of abnormality",
              "Characteristics of phobias, depression, OCD",
              "Behavioural, cognitive, and biological explanations of disorders",
              "Treatments (systematic desensitisation, CBT, drug therapy)"
            ]
          }
        ]
      },
      {
        "Name": "Paper 2",
        "Title": "Psychology in Context",
        "Topics": [
          {
            "Title": "Approaches in Psychology",
            "Content": [
              "Origins of psychology (Wundt)",
              "Behaviourist approach",
              "Social learning theory",
              "Cognitive approach",
              "Biological approach",
              "Psychodynamic approach (Freud)",
              "Humanistic approach"
            ]
          },
          {
            "Title": "Biopsychology",
            "Content": [
              "Nervous system and endocrine system",
              "Fight or flight response",
              "Localisation of function in the brain",
              "Plasticity and functional recovery",
              "Biological rhythms (circadian, infradian, ultradian)"
            ]
          },
          {
            "Title": "Research Methods",
            "Content": [
              "Experimental methods",
              "Correlations, observations, self-report",
              "Ethics in psychological research",
              "Statistical tests (chi-square, t-test, Mann-Whitney)"
            ]
          }
        ]
      },
      {
        "Name": "Paper 3",
        "Title": "Issues and Options in Psychology",
        "Topics": [
          {
            "Title": "Issues and Debates in Psychology",
            "Content": [
              "Gender and culture bias",
              "Free will vs. determinism",
              "Nature vs. nurture",
              "Holism vs. reductionism",
              "Idiographic vs. nomothetic approach",
              "Ethical implications and social sensitivity"
            ]
          },
          {
            "Title": "Relationships",
            "Content": [
              "Evolutionary explanations of partner preference (sexual selection)",
              "Factors affecting attraction (self-disclosure, physical attractiveness, filter theory)",
              "Theories of romantic relationships (social exchange theory, equity theory, Rusbult's investment model)",
              "Duck's phase model of relationship breakdown",
              "Virtual relationships in social media",
              "Parasocial relationships (celebrity worship, absorption-addiction model, attachment theory)"
            ]
          },
          {
            "Title": "Schizophrenia",
            "Content": [
              "Classification of schizophrenia (ICD-10, DSM-5, reliability and validity issues)",
              "Biological explanations (genetics, dopamine hypothesis, neural correlates)",
              "Psychological explanations (family dysfunction, cognitive explanations)",
              "Drug therapy (typical and atypical antipsychotics)",
              "Psychological therapies (CBT, family therapy, token economies)",
              "The interactionist approach (diathesis-stress model)"
            ]
          },
          {
            "Title": "Forensic Psychology",
            "Content": [
              "Defining and measuring crime",
              "Offender profiling (top-down and bottom-up approaches)",
              "Biological explanations (atavistic form, genetic and neural explanations)",
              "Psychological explanations (Eysenck's theory, cognitive explanations, differential association theory, psychodynamic explanations)",
              "Dealing with offending behaviour (custodial sentencing, behaviour modification, anger management, restorative justice)"
            ]
          }
        ]
      }
    ]
  },
